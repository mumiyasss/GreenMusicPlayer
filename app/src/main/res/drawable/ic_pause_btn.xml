<vector
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:width="106dp"
        android:height="106dp"
        android:viewportWidth="128"
        android:viewportHeight="128">

    <path android:fillColor="#43A047" android:fillType="nonZero"
          android:pathData="M64,64m-64,0a64,64 0,1 1,128 0a64,64 0,1 1,-128 0"
          android:strokeColor="#00000000" android:strokeWidth="1"/>

    <group
            android:name="pause_icon_group_parts"
            android:pivotX="64"
            android:pivotY="64"
            android:scaleX="1"
            android:scaleY="1"
            android:rotation="90">

        <group android:name="pause_icon_group_top">
            <path
                    android:name="pause_icon_top_path_name"
                    android:pathData="@string/pause_icon_upper_path_data"
                    android:fillColor="#FFF"
                    android:strokeLineCap="butt"
                    android:strokeLineJoin="miter"
                    android:strokeMiterLimit="10"/>
        </group>
        <group android:name="pause_icon_group_bottom">
            <path
                    android:name="pause_icon_bottom_path_name"
                    android:pathData="@string/pause_icon_bottom_path_data"
                    android:fillColor="#FFF"
                    android:strokeLineCap="butt"
                    android:strokeLineJoin="miter"
                    android:strokeMiterLimit="10"/>
        </group>

    </group>
</vector>